{
    "name": "vscode-tree-sitter-playground",
    "displayName": "vscode-tree-sitter-playground",
    "publisher": "kkkqkx123",
    "description": "Tools for working with Tree-Sitter",
    "version": "0.0.1",
    "repository": {
        "url": "https://github.com/kkkqkx123/tree-sitter-playground"
    },
    "engines": {
        "vscode": "^1.84.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:json",
        "onNotebook:tree-sitter-query",
        "workspaceContains:**/*.tsqnb"
    ],
    "main": "./dist/extension.js",
    "browser": "./dist/web/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "scm",
                "aliases": [
                    "Tree-sitter Query",
                    "scm"
                ],
                "extensions": [
                    ".scm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "scm",
                "scopeName": "source.scm",
                "path": "./syntaxes/scm.tmLanguage.json"
            }
        ],
        "notebooks": [
            {
                "type": "tree-sitter-query",
                "displayName": "Tree-sitter Query Notebook",
                "selector": [
                    {
                        "filenamePattern": "*.tsqnb"
                    }
                ]
            }
        ],
        "notebookEditor": {
            "viewType": "tree-sitter-query",
            "displayName": "Tree-sitter Query Notebook",
            "selector": [
                {
                    "filenamePattern": "*.tsqnb"
                }
            ]
        },
        "files": {
            "associations": {}
        },
        "notebookRenderer": [
            {
                "id": "tree-sitter-notebook",
                "displayName": "Tree-Sitter Output Renderer",
                "mimeTypes": [
                    "x-application/tree-sitter"
                ],
                "requiresMessaging": "always",
                "entrypoint": "./dist/renderer.js"
            }
        ],
        "commands": [
            {
                "command": "vscode-treesitter-notebook.new",
                "title": "Tree-Sitter Notebook",
                "category": "Tree-Sitter Query"
            },
            {
                "command": "vscode-treesitter-parse-tree-editor.createToSide",
                "title": "Open Parse Tree View to Side",
                "category": "Tree-Sitter"
            }
        ],
        "menus": {
            "file/newFile": [
                {
                    "command": "vscode-treesitter-notebook.new"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "npm run build-wasm && webpack --mode production --devtool hidden-source-map",
        "build": "webpack --mode production --devtool hidden-source-map && npx vsce package",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "build-wasm": "ts-node src/buildWasm.ts && copy node_modules\\web-tree-sitter\\tree-sitter.wasm src\\.wasm\\ && copy src\\.wasm\\*.wasm dist\\ 2>nul || (exit 0)"
    },
    "devDependencies": {
        "@types/node": "24.10.1",
        "@types/vscode": "^1.84.0",
        "@types/vscode-notebook-renderer": "^1.72.4",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "@vscode/test-electron": "^2.5.2",
        "@vscode/vsce": "^3.7.0",
        "copy-webpack-plugin": "^13.0.1",
        "eslint": "^9.39.1",
        "glob": "^11.0.3",
        "mocha": "^11.7.5",
        "path-browserify": "^1.0.1",
        "tree-sitter-c-sharp": "^0.23.1",
        "tree-sitter-cli": "^0.25.10",
        "tree-sitter-cpp": "^0.23.4",
        "tree-sitter-go": "^0.25.0",
        "tree-sitter-java": "^0.23.5",
        "tree-sitter-javascript": "^0.25.0",
        "tree-sitter-python": "^0.25.0",
        "tree-sitter-ruby": "^0.23.1",
        "tree-sitter-rust": "^0.24.0",
        "tree-sitter-typescript": "^0.23.2",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "web-tree-sitter": "^0.25.10",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "jsonc-parser": "^3.3.1"
    }
}
